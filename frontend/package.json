{
  "name": "euamocerrado-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve-map-dev": "npm link vue-mapbox && vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-report": "vue-cli-service build --report && open dist/report.html ",
    "lint": "vue-cli-service lint",
    "generate-icons": "icon-font-generator -p vtl -t i --descent  10 --normalize --height 50  --csstp svg-icons/css.hbs --htmltp svg-icons/html.hbs svg-icons/*.svg -o public/icon-dist",
    "generate-sprites": "spritezero images/sprites svg-sprites --ratio=4; cp images/sprites.json images/sprites@2x.json;  cp images/sprites.png images/sprites@2x.png; ",
    "deploy": "rsync -av -e \"ssh -p 22022\" --exclude .git --exclude node_modules ./dist/ euamocerrado@hex2.dev.br:~/public_html/"
  },
  "dependencies": {
    "@babel/compat-data": "^7.12.7",
    "@curupira/vue-mapbox": "^0.1.1-beta.42",
    "@mapbox/mapbox-gl-draw": "^1.1.2",
    "@turf/along": "^6.0.1",
    "@turf/bbox": "^6.0.1",
    "@turf/center-of-mass": "^6.0.1",
    "@turf/circle": "^6.0.1",
    "@turf/destination": "^6.0.1",
    "@turf/distance": "^6.0.1",
    "@turf/invariant": "^6.1.2",
    "@turf/length": "^6.0.2",
    "@turf/line-slice": "^5.1.5",
    "@turf/line-slice-along": "^5.1.5",
    "@turf/nearest-point-on-line": "^6.0.2",
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.0.4",
    "core-js": "^3.6.4",
    "flickity": "^2.2.1",
    "flickity-imagesloaded": "^2.0.0",
    "fuse.js": "^3.2.0",
    "lodash": "^4.17.15",
    "popper.js": "^1.12.5",
    "tippy.js": "^1.4.0",
    "tooltip.js": "^1.1.5",
    "vue": "^2.6.11",
    "vue-notification": "^1.3.20",
    "vue-popperjs": "^2.3.0",
    "vue-router": "^3.1.6",
    "vue-tippy": "^4.4.0",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.11.1",
    "@babel/preset-flow": "^7.10.4",
    "@vue-flow-typed/vuex": "^0.4.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "icon-font-generator": "^2.1.11",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-compression": "~1.1.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
