{
  "name": "mapa_da_saude_i3geo",
  "description": "Projeto de interface para i3geo",
  "version": "1.0.0",
  "author": "Leandro Celes <leandroceles@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "devTracker": "cross-env NODE_ENV=development webpack-dev-server --config webpack-tracker.config.js --hot",
    "devDashboard": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --hot ",
    "generate-sprites": "spritezero images/sprites svg-sprites --ratio=4; cp images/sprites.json images/sprites@2x.json;  cp images/sprites.png images/sprites@2x.png; ",
    "generate-icons": "icon-font-generator -p vtl -t i --descent  10 --normalize --height 50  --csstp svg-icons/css.hbs --htmltp svg-icons/html.hbs svg-icons/*.svg -o icon-dist",
    "build": "NODE_ENV=production webpack --progress",
    "buildDashboard": "cross-env NODE_ENV=production  webpack-dashboard -- webpack --progress",
    "svg": "vsvg -s src/svgs/ -t src/icons",
    "buildDeploy": "cross-env NODE_ENV=production webpack --progress; rsync ./ -av --exclude 'node_modules' --exclude 'src/js-vtl' --exclude 'undercover' --exclude 'server' --exclude '.git'  --exclude 'LAYOUT' euamocerradocom@curupiradesign.com.br:~/public_html/",
    "deploy": "rsync ./ -av --exclude 'node_modules' --exclude 'src/js-vtl' --exclude 'undercover' --exclude 'server' --exclude '.git'  --exclude 'LAYOUT' euamocerradocom@curupiradesign.com.br:~/public_html/"
  },
